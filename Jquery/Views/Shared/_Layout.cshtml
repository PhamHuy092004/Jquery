<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0" />
    <title>@ViewData["Title"] - Jquery</title>
    <link rel="stylesheet" href="~/lib/bootstrap/dist/css/bootstrap.min.css" />
    <link rel="stylesheet" href="~/css/site.css" asp-append-version="true" />
    <link rel="stylesheet" href="~/Jquery.styles.css" asp-append-version="true" />
    <!-- Global stylesheets -->
    <link href="../../css/assets/inter.css" rel="stylesheet" type="text/css">
    <link href="~/css/assets/styles.min.css" rel="stylesheet" type="text/css">
    <link href="../css/assets/all.min.css" id="stylesheet" rel="stylesheet" type="text/css">
    <!-- Core JS files -->
    <script src="../../../js/demo_configurator.js"></script>
    <script src="../../../js/bootstrap.bundle.min.js"></script>
    <!-- Theme JS files -->
    <script src="../../../js/theme/d3.min.js"></script>
    <script src="../../../js/theme/d3_tooltip.js"></script>

    <script src="../js/app.js"></script>
    <script src="../../../js/demo/dashboard.js"></script>
    <script src="../../../js/demo/streamgraph.js"></script>
    <script src="../../../js/demo/sparklines.js"></script>
    <script src="../../../js/demo/lines.js"></script>
    <script src="../../../js/demo/areas.js"></script>
    <script src="../../../js/demo/donuts.js"></script>
    <script src="../../../js/demo/bars.js"></script>
    <script src="../../../js/demo/progress.js"></script>
    <script src="../../../js/demo/heatmaps.js"></script>
    <script src="../../../js/demo/pies.js"></script>
    <script src="../../../js/demo/bullets.js"></script>
    <script src="https://cdn.jsdelivr.net/npm/phosphor-icons@1.4.2/src/index.min.js"></script>
    <link rel="stylesheet" href="https://cdn.jsdelivr.net/npm/phosphor-icons@1.4.2/src/css/icons.min.css">
</head>
<body>
 
    @Html.Partial("_Header");
    <div class="container" style="margin-bottom:500px">
        <main role="main" class="pb-3">
            @RenderBody()
        </main>
    </div>
    @Html.Partial("_Footer");

 
    <script src="~/lib/jquery/dist/jquery.min.js"></script>
    <script src="~/js/site.js" asp-append-version="true"></script>
    @await RenderSectionAsync("Scripts", required: false)
</body>
</html>
